{
  "forge_marker": 1,
  "defaults": {
    "textures": {
      "#chest": "entity/chest/normal",
      "base": "blocks/stone",
      "front": "blocks/log_oak",
      "chamber": "blocks/redstone_block",
      "trunk": "blocks/end_stone"
    },
        "model": "examplemod:chest.ogex",
        "custom": {
            "mesh": ["Base", "Lid"]
        }
  },
  "variants": {
    "normal": [{}],
    "inventory": [{
      "transform": "forge:default-block"
